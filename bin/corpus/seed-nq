#! /bin/sh

while read p
do
(
	GOPATH=var/go bin/pkg/deps $p | while read d
	do
		echo "<$p> <depends> <$d>."
	done
)
done < packages/seed

while read p
do
(
	if [ $(GOPATH=var/go bin/pkg/name $p) = 'main' ]
	then
		echo "<$p> <is> <cmd>."
	else
		echo "<$p> <is> <lib>."
	fi
)
done < packages/seed

while read p
do
	echo "<$p> <in> <stdlib>."
done < packages/stdlib
